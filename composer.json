{
    "name": "nihilo/nova-logs-tool",
    "description": "A Laravel Nova tool to manage and keep track of each one of your logs files.",
    "keywords": [
        "laravel",
        "nova"
    ],
    "homepage": "https://github.com/Nihilop/nova-logs-tool",
    "license": "MIT",
    "authors": [
        {
            "name": "Nihilo",
            "email": "contact@nihilo.fr",
            "role": "Developer"
        }
    ],
    "require": {
        "php": ">=8.0.0",
        "laravel/nova": "^4.0",
        "stepanenko3/laravel-log-viewer": "*"
    },
    "repositories": [
        {
            "type": "path",
            "url": "../nova"
        }
    ],
    "autoload": {
        "psr-4": {
            "Stepanenko3\\LogsTool\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Stepanenko3\\LogsTool\\LogsToolServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
